@model CoreTestPl.WievModels.HomePageViewModel

<html xmlns="http://www.w3.org/1999/xhtml">
<head>
    <title>Restaurans</title>
</head>
<body>
    <h1>@Model.CurrentMessageOfTheDay</h1> 
    <table>
        @foreach (var restaurant in Model.Restaurants)
        {
            <tr>
                <td>@restaurant.Id</td>
                <td>@restaurant.Name</td>
            </tr>
            
        }
    </table>
   
</body>
</html>